
<div class="page login-page">
        <div class="container">
          <div class="form-outer text-center d-flex align-items-center">
            <div class="form-inner">
              <div class="logo text-uppercase"><span>Dash</span><strong class="text-primary">Express</strong></div>
              {{#if message}}
              <div class="alert alert-info">{{message}}</div>
              {{/if}}
              <form id="register-form" method="post" action="signup">
                <div class="form-group">
                  <label for="register-email" class="label-custom">Username      </label>
                  <input id="register-email" type="text" name="username" required>
                </div>
                <div class="form-group">
                  <label for="register-passowrd" class="label-custom">password        </label>
                  <input id="register-passowrd" type="password" name="password" required>
                </div>
                <input id="register" type="submit" value="Register" class="btn btn-primary">
              </form>
              <small>Already have an account? </small>
              <a href="/admin/login" class="signup">Login</a>
            </div>
            <div class="copyrights text-center">
              <p>Design by <a href="https://bootstrapious.com" class="external">Bootstrapious</a></p>
              <!-- Please do not remove the backlink to us unless you support further theme's development at https://bootstrapious.com/donate. It is part of the license conditions. Thank you for understanding :)-->
            </div>
          </div>
        </div>
      </div>